> #산점도2
> plot(x=survey$Grade, y=survey$Age, xlab="학년", ylab="나이", main="학년와 나이의 변화", pch=24, col="red", bg="yellow", cex=1.5)
> #산점도3
> plot(x=survey$Grade, y=survey$Age, xlab="학년", ylab="나이", main="학년와 나이의 변화", type = "h")