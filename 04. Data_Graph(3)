> library(readxl)
> survey <- read_excel("04. 2025STB_survey.xlsx")
> View(survey)

# table()함수/구분 1개의 인자를 가지고 도수분포표 작성
table(survey$Gender)#성별

# table()함수/구분 1개의 인자를 가지고 상대도수분포표를 작성
ECN <- table(survey$Gender)#ECN에 성별의 도수분포표를 대입
prop.table(ECN)#상대 도수로 표시

# table()함수/구분 2개의 인자를 가지고 교차표를 작성
table(survey$Gender, Ksurvey$Grade)

#막대그래프1 
barplot(table(survey$Nationality))

# table()함수/구분 1개의 인자를 가지고 (가로)막대그래프를 작성
barplot(table(survey$`Residential area`))#주거지역

#누적-막대그래프
entry <- table(survey$Gender, survey$Grade)
barplot(entry, legend = TRUE)

#파이차트1
pie(table(survey$Grade))

#히스토그램 작성1
hist(survey$Age,col=terrain.colors(12))

